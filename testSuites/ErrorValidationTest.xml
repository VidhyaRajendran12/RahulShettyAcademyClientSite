<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel='tests' name="E-Commerce Functional Test">

	<groups>

		<run>
			<include name="ErrorHandling"></include>
		</run>


	</groups>


	<test thread-count="5" name="SubmitOrderTest">
		<classes>
			<class name="com.test.SubmitOrder" />


		</classes>
	</test>
	<test thread-count="5" name="ErrorValidationTest">
		<classes>
			<class name="com.test.ErrorValidation" />

		</classes>
	</test>
</suite> 
